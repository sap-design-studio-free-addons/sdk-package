
<component
	id="RangeSlider"
	title="Data Range Slider"
	icon="res/rsl/DRangeSlider.png"
	handlerType="sapui5"
	group="ScnCommunityDataBound"
	databound="true"
>

	<stdInclude kind="cvom" />

	<jsInclude>res/rscommon.js</jsInclude>
	<jsInclude>res/rsl/DRangeSlider.js</jsInclude>
	<cssInclude>res/rsl/DRangeSlider.css</cssInclude>

	<property
		id="data"
		title="Data List"
		type="ResultCellList"
		group="DataBinding"
	>
	</property>

	<property
		id="maxNumber"
		title="Maximum Number"
		type="int"
		group="Display" />

	<property
		id="topBottom"
		title="Top X, Bottom X or Both"
		type="String"
		group="Display"
	>
		<possibleValue>Top X</possibleValue>
		<possibleValue>Bottom X</possibleValue>
		<possibleValue>Both</possibleValue>
	</property>

	<property
		id="sorting"
		title="Sorting By Value | Defalut"
		type="String"
		group="Display"
	>
		<possibleValue>By Value</possibleValue>
		<possibleValue>Default</possibleValue>
	</property>

	<property
		id="vertical"
		title="Change to Vertical Orientation"
		type="boolean"
		group="Display"
		visible="false" />

	<property
		id="doRefresh"
		type="boolean"
		title="Technical proprty for pause refresh"
		group="Display"
		visible="false" />

	<property
		id="selectedKey"
		type="String"
		title="Technical proprty for Selected Key"
		group="Display"
		visible="false" />

	<property
		id="selectedText"
		type="String"
		title="Technical proprty for Selected Text"
		group="Display"
		visible="false" />

	<property
		id="selectedKey2"
		type="String"
		title="Technical proprty for Selected Key 2"
		group="Display"
		visible="false" />

	<property
		id="selectedText2"
		type="String"
		title="Technical proprty for Selected Text 2"
		group="Display"
		visible="false" />

	<property
		id="selectedKeys"
		type="String"
		title="Technical proprty for Selected Keys"
		group="Display"
		visible="false" />

	<property
		id="onSelectionChanged"
		type="ScriptText"
		title="Event for On Selection Changed"
		group="Events" />

	<property
		id="onDataChanged"
		type="ScriptText"
		title="Event for On Data Changed"
		group="Events" />

	<initialization>
		<defaultValue property="TOP">0</defaultValue>
		<defaultValue property="LEFT">0</defaultValue>
		<defaultValue property="WIDTH">60</defaultValue>
		<defaultValue property="HEIGHT">600</defaultValue>

		<defaultValue property="topBottom">Both</defaultValue>
		<defaultValue property="sorting">Default</defaultValue>
		<defaultValue property="maxNumber">10</defaultValue>
		<defaultValue property="vertical">false</defaultValue>
		<defaultValue property="doRefresh">true</defaultValue>

	</initialization>

</component>